#!/usr/bin/env enter-mode
~/.config/river/init	reload
riverctl exit	x
sh -c "$(bemenu < /dev/null)"	e
open-mode	o
script-mode	s
tile-mode	t
newterm sh -c 'START_COMMAND="kak -c \\$(list-kak | bemenu)" exec bash'	k
printf 'kill!' | kak -p "$(kak -l | bemenu -p "kill")"	q
newterm kak "$(locate-note "$(rg -Ir\$1 "^# (.*)" "$BRAIN_DIR" | sort | bemenu -il 20)")"	n
newterm sudo zzz	p
