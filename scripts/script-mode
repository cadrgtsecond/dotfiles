#!/usr/bin/env enter-mode
cat ~/.local/bin/scripts/unicode_list.csv | bemenu -l 20 | cut -d, -f1 | wl-copy -n	e
flavours list | tr ' ' '\n' | floatpipe fzf --preview 'flavours apply {}'	t
kak -l | xargs -I{} kak -c {} -e kill!	q
change-keyboard $(bemenu -l 20 <~/.config/keyboards)	b
apk search | floatpipe fzf | wl-copy	pr
apk list -I | floatpipe fzf | wl-copy	pi
cat /etc/apk/world | floatpipe fzf | wl-copy	pm
floatterm doas sh -c 'while true; do kak /etc/apk/world; apk upgrade && break; printf failure...; read; done; printf done...; read'	w
floatterm doas sh -c 'apk upgrade -U; printf done...; read'	u
