#!/usr/bin/env enter-mode
cat ~/.local/bin/scripts/unicode_list.csv | bemenu | cut -d, -f1 | wl-copy -n	e
flavours list | tr ' ' '\n' | floatpipe fzf --preview 'flavours apply {}'	t
kak -l | xargs -I{} kak -c {} -e kill!	q
change-keyboard $(bemenu <~/.config/keyboards)	b
xbps-query -Rs '' | floatpipe fzf	pr
xbps-query -s '' | floatpipe fzf	pi
xbps-query -m | floatpipe fzf	pm
